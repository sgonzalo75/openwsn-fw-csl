 216:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_SSI1_TXD
 217:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_SSI1_CLKOUT
 218:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_SSI1_FSSOUT
 219:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_SSI1_STXSER_EN
 220:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_I2C_CMSSDA
 221:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_I2C_CMSSCL
 222:bsp/boards/OpenMote-CC2538/source/ioc.c **** //! - \b IOC_MUX_OUT_SEL_GPT0_ICP1
